
@{
    ViewBag.Title = "LeaderboardsView";
}

<h2>Leaderboards</h2>


<table class="table">
    <tr>
        <th>
            User
        </th>
        <th>
            Observation Count
        </th>
    </tr>

    @for (int i = 0; i < ViewBag.UserList.Count; i++)
    {
        <tr>
            <td>
                @ViewBag.UserList[i]
            </td>
            <td>
                @ViewBag.UserCount[i]
            </td>
        </tr>
    }
</table>